
<% if user_signed_in? %>
  <strong><h1 class="text_center">Hello <%= @current_user %>!</h1></strong>.
  <p  class="text_center"><%= link_to 'Edit profile', edit_user_registration_path, :class => 'navbar-link' %> |
  <%= link_to "Logout", destroy_user_session_path, data: { turbo_method: :delete, confirm: "Are you sure?", turbo_confirm: "Are you sure?" }, :class => 'navbar-link'  %></p>
<% else %>
<h1 class="text_center">Welcome, Guest</h1>
<% end %>
